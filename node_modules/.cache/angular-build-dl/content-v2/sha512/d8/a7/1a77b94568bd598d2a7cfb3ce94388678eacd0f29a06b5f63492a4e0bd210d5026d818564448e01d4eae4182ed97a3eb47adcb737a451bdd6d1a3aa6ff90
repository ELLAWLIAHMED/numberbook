let LoginComponent = class LoginComponent {
    constructor(firebaservice, router) {
        this.firebaservice = firebaservice;
        this.router = router;
        this.error = null;
    }
    ngOnInit() {
        this.firebaservice.signout();
    }
    connect() {
        this.firebaservice.login(this.email, this.password).then(() => {
            if (this.firebaservice.authState != null) {
                this.router.navigate(['folder/Inbox']);
                this.email = "";
                this.password = "";
            }
            else {
                this.error = "invalid informations";
                this.password = "";
            }
        });
    }
};
